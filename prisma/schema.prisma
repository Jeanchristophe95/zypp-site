generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RechargeurApplication {
  id             Int      @id @default(autoincrement())
  fullName       String
  email          String
  phone          String
  city           String
  availability   String
  note           String?
  createdAt      DateTime @default(now())

  @@unique([email, createdAt])
}


